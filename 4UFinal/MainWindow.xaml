<Window x:Name="OuterScreen" x:Class="_4UFinal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_4UFinal"
        mc:Ignorable="d" Height="800" Width="1000" MinWidth="1000" MinHeight="800">
    <Window.Background>
        <ImageBrush/>
    </Window.Background>
    <Grid>
        <Canvas x:Name="InnerScreen" HorizontalAlignment="Center" Height="800" Margin="0" VerticalAlignment="Center" Width="1000" Background="Black"/>
    </Grid>
</Window>
